{
  "name": "lil-pilchuck-creek-farm",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "index": "node --enable-source-maps ./scripts/indexDir.js",
    "preindex": "tsc --sourceMap ./scripts/indexDir.ts",
    "add-image": "node --enable-source-maps ./scripts/addImages.js",
    "preadd-image": "tsc --sourceMap ./scripts/addImages.ts",
    "postadd-image": "npm run index",
    "sync": "node --enable-source-maps ./scripts/sync.js",
    "presync": "tsc --sourceMap ./scripts/sync.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.6",
    "@angular/common": "^15.2.6",
    "@angular/compiler": "^15.2.6",
    "@angular/core": "^15.2.6",
    "@angular/forms": "^15.2.6",
    "@angular/platform-browser": "^15.2.6",
    "@angular/platform-browser-dynamic": "^15.2.6",
    "@angular/router": "^15.2.6",
    "adga": "^3.1.1",
    "bootstrap": "5.3.0-alpha3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.5",
    "@angular/compiler-cli": "^15.2.6",
    "@types/bootstrap": "^5.2.5",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "axios": "^0.27.2",
    "directory-tree": "^3.4.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-safari-launcher": "^1.0.0",
    "typescript": "~4.9.5"
  }
